{
  "name": "pr-tagger",
  "version": "0.0.0",
  "description": "Add comment to PRs in GitHub with version tag for latest release",
  "main": "pr-tagger.js",
  "bin": {
    "pr-tagger": "./pr-tagger.js"
  },
  "scripts": {
    "cover": "istanbul cover _mocha -- test.js",
    "coveralls": "istanbul cover --report lcovonly _mocha && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "standard",
    "postversion": "git push && git push --tags",
    "pretest": "npm run lint",
    "preversion": "npm test",
    "test": "mocha test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcollado/pr-tagger.git"
  },
  "keywords": [
    "PR",
    "tag",
    "github",
    "release"
  ],
  "author": "Javier Collado <javier.collado@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcollado/pr-tagger/issues"
  },
  "homepage": "https://github.com/jcollado/pr-tagger#readme",
  "dependencies": {
    "commander": "2.9.0",
    "ghauth": "3.0.0",
    "ghissues": "1.1.2",
    "github-url": "1.2.0",
    "semver-regex": "1.0.0",
    "winston": "2.1.1"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "coveralls": "2.11.4",
    "ghooks": "1.0.1",
    "istanbul": "0.4.1",
    "mocha": "2.3.4",
    "require-inject": "1.3.0",
    "rewire": "2.5.0",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0",
    "standard": "5.4.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  }
}
