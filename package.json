{
  "name": "pr-tagger",
  "version": "0.7.1",
  "description": "Add comment to PRs in GitHub with version tag for latest release",
  "main": "./lib/main",
  "bin": {
    "pr-tagger": "./pr-tagger.js"
  },
  "scripts": {
    "build": "babel -d lib src",
    "cover": "nyc npm test",
    "coveralls": "npm run cover && npm run report:text | coveralls",
    "lint": "standard",
    "multitest": "multitest",
    "postversion": "git push --follow-tags",
    "premultitest": "npm run lint",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "preversion": "npm run multitest",
    "report:html": "nyc report --reporter html",
    "report:text": "nyc report --reporter text-lcov",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcollado/pr-tagger.git"
  },
  "keywords": [
    "PR",
    "tag",
    "github",
    "release"
  ],
  "author": "Javier Collado <javier.collado@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcollado/pr-tagger/issues"
  },
  "homepage": "https://github.com/jcollado/pr-tagger#readme",
  "dependencies": {
    "application-config": "0.2.0",
    "commander": "2.9.0",
    "ghauth": "3.2.0",
    "ghissues": "1.1.2",
    "github-url": "1.2.0",
    "promisify-object": "1.2.1",
    "semver": "5.1.0",
    "semver-regex": "1.0.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "ava": "0.13.0",
    "babel-cli": "6.6.5",
    "babel-eslint": "5.0.1",
    "babel-preset-es2015-node4": "2.0.3",
    "babel-register": "6.6.5",
    "coveralls": "2.11.9",
    "ghooks": "1.0.3",
    "multitest": "0.4.1",
    "nyc": "6.0.0",
    "require-inject": "1.3.1",
    "sinon": "1.17.3",
    "sinon-as-promised": "4.0.0",
    "standard": "6.0.8"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test",
      "pre-push": "npm run multitest"
    }
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
