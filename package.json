{
  "name": "pr-tagger",
  "version": "0.7.1",
  "description": "Add comment to PRs in GitHub with version tag for latest release",
  "main": "./lib/main",
  "bin": {
    "pr-tagger": "./pr-tagger.js"
  },
  "scripts": {
    "build": "babel -d lib src",
    "cover": "nyc npm test",
    "coveralls": "npm run cover && npm run report:text | coveralls",
    "lint": "standard",
    "multitest": "multitest",
    "postversion": "git push --follow-tags",
    "premultitest": "npm run lint",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "preversion": "npm run multitest",
    "report:html": "nyc report --reporter html",
    "report:text": "nyc report --reporter text-lcov",
    "test": "mocha --compilers js:babel-register --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcollado/pr-tagger.git"
  },
  "keywords": [
    "PR",
    "tag",
    "github",
    "release"
  ],
  "author": "Javier Collado <javier.collado@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcollado/pr-tagger/issues"
  },
  "homepage": "https://github.com/jcollado/pr-tagger#readme",
  "dependencies": {
    "application-config": "0.2.0",
    "commander": "2.9.0",
    "ghauth": "3.2.0",
    "ghissues": "1.1.2",
    "github-url": "1.2.0",
    "promisify-object": "1.2.1",
    "semver": "5.1.0",
    "semver-regex": "1.0.0",
    "winston": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.5.0",
    "babel-preset-es2015-node4": "2.0.3",
    "babel-register": "6.5.1",
    "chai": "3.5.0",
    "chai-as-promised": "5.2.0",
    "coveralls": "2.11.6",
    "ghooks": "1.0.3",
    "mocha": "2.4.5",
    "multitest": "0.4.1",
    "nyc": "5.6.0",
    "require-inject": "1.3.0",
    "sinon": "1.17.3",
    "sinon-as-promised": "4.0.0",
    "sinon-chai": "2.8.0",
    "standard": "6.0.4"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test",
      "pre-push": "npm run multitest"
    }
  },
  "babel": {
    "presets": [
      "es2015-node4"
    ]
  }
}
